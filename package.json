{
  "name": "pkg-placeholder",
  "type": "module",
  "version": "0.0.0",
  "packageManager": "pnpm@9.1.1",
  "description": "_description_",
  "author": "Anthony Fu <anthonyfu117@hotmail.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/antfu",
  "homepage": "https://github.com/antfu/pkg-placeholder#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/antfu/pkg-placeholder.git"
  },
  "bugs": "https://github.com/antfu/pkg-placeholder/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "pnpm -C packages/core dev",
    "build": "pnpm -C packages/core build",
    "lint": "eslint .",
    "release": "bumpp && npm publish",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.18.1",
    "@antfu/ni": "^0.21.12",
    "@antfu/utils": "^0.7.8",
    "@types/node": "^20.12.12",
    "@unocss/eslint-plugin": "^0.61.0",
    "bumpp": "^9.4.1",
    "eslint": "^9.3.0",
    "esno": "^4.7.0",
    "lint-staged": "^15.2.2",
    "pnpm": "^9.1.1",
    "rimraf": "^5.0.7",
    "simple-git-hooks": "^2.11.1",
    "typescript": "^5.4.5",
    "unbuild": "^2.0.0",
    "vite": "^5.2.11",
    "vitest": "^1.6.0"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  },
  "dependencies": {
    "@iconify/json": "^2.2.224",
    "@julr/unocss-preset-forms": "^0.1.0",
    "@kobalte/core": "^0.13.3",
    "@unocss/reset": "^0.61.0",
    "code-inspector-plugin": "^0.14.2",
    "eslint-plugin-solid": "^0.14.1",
    "fast-glob": "^3.3.2",
    "solid-js": "^1.8.18",
    "tailwind-merge": "^2.3.0",
    "unocss": "^0.61.0",
    "unocss-preset-animations": "^1.0.2",
    "unocss-preset-extra": "^0.5.3",
    "vite-plugin-dts": "^3.9.1"
  }
}
